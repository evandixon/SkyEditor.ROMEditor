<skywpf:DataBoundObjectControl x:Class="PsmdLuaLangIntegration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkyEditor.ROMEditor.UI.WPF"
             xmlns:vm="clr-namespace:SkyEditor.ROMEditor.UI.WPF.ViewModels"
             xmlns:skywpf="clr-namespace:SkyEditor.UI.WPF;assembly=SkyEditor.UI.WPF"
             xmlns:res="clr-namespace:SkyEditor.ROMEditor.UI.WPF.My.Resources"
             TargetType="{x:Type vm:PsmdLuaLangIntegrationViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl x:Name="tcTabs" Margin="0,0,0,31" />
        <Button x:Name="btnAdd" Content="{x:Static res:Language.Add}" HorizontalAlignment="Left" Margin="10,0,0,6" VerticalAlignment="Bottom" Width="100"/>
        <Button x:Name="btnSciptSort" Content="{x:Static res:Language.ScriptSort}" HorizontalAlignment="Left" Margin="115,0,0,6" VerticalAlignment="Bottom" Width="100" />
    </Grid>
</skywpf:DataBoundObjectControl>
